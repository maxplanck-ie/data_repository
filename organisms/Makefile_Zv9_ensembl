genome_url=ftp://ftp.ensembl.org/pub/release-78/fasta/danio_rerio/dna/Danio_rerio.Zv9.dna.toplevel.fa.gz
gene_gtf_tuple=\
	ftp$$ftp.ensembl.org/pub/release-78/gtf/danio_rerio/Danio_rerio.Zv9.78.gtf.gz$$ensembl$$release-78

include ../Makefile.inc

UCSC/danRer7/rmsk.txt:
	mkdir -p $(dir $@)
	mysql --user=genome --host=genome-mysql.cse.ucsc.edu -D danRer7 -e "select * from rmsk" \
	    | sed -e "s/\|//g" -e "s/^bin/#bin/" \
	    | ../convertChromosomes.py -n 6 UCSC.map > $@

OTHER: UCSC/danRer7/rmsk.txt
